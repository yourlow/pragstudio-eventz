<ul>
  <% @events.each do |event|%>
    <li>
      <strong><%= event.name %></strong>
      in <%= event.location%>
      <%= price(event) %>
      <p>
        <%= truncate(event.description, length: 35, seperator: ' ') %>
      </p>
      <p>
        <%= pretty_date(event) %>
      </p>
    </li>
  <% end %>
</ul>